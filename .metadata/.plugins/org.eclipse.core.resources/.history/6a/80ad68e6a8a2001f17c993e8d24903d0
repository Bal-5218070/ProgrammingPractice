package collection;

import java.util.ArrayList;
import java.util.Collection;

public class Program3 {
public static void main(String[] args) {
	Collection c1= new ArrayList();
	c1.add("sharma");
	c1.add(20);
	c1.add("kishore");
	c1.add(8);
	c1.add(null);
	c1.add(true);
	System.out.println(c1);
	Object[] k = c1.toArray();
	 
	  System.out.println(k); 
}
}
